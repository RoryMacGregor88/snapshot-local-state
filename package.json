    "tailwindcss": "^3.2.4",
    "tailwindcss-debug-screens": "^2.2.1",
    "typescript": "^5.0.2",
    "vite": "^4.2.0",
    "vitest": "^0.29.3"
  },
  "msw": {
    "workerDirectory": ""
  }
