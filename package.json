{
  "name": "business-directory",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite --open",
    "dev:local": "VITE_USE_MSW=true vite --open",
    "test": "vitest watch",
    "test:ci": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "cypress open",
    "test:e2e:run": "cypress run",
    "postinstall": "husky install",
    "verify": "concurrently --raw 'eslint src/**.{ts,tsx}' 'tsc' 'yarn test:ci'",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "storybook": "NODE_OPTIONS=--openssl-legacy-provider start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "husky": {
    "hooks": {
      "pre-push": "yarn verify"
    }
  },
  "dependencies": {
    "@astrosat/react-utils": "^1.1.1",
    "@tanstack/react-query": "^4.26.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "cross-fetch": "^3.1.5",
    "daisyui": "^2.42.1",
    "i18next": "^22.0.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^3.1.4",
    "react-i18next": "^12.0.0",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@astrosat/eslint-config-astrosat": "^1.1.3",
    "@astrosat/prettier-config": "^1.1.2",
    "@astrosat/semantic-release-config": "^1.1.2",
    "@axe-core/react": "^4.5.2",
    "@babel/core": "^7.20.5",
    "@storybook/addon-a11y": "^6.5.13",
    "@storybook/addon-actions": "^6.5.13",
    "@storybook/addon-essentials": "^6.5.13",
    "@storybook/addon-interactions": "^6.5.13",
    "@storybook/addon-links": "^6.5.13",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/builder-vite": "^0.4.2",
    "@storybook/react": "^6.5.13",
    "@storybook/testing-library": "^0.0.13",
    "@tanstack/react-query-devtools": "^4.26.1",
    "@testing-library/cypress": "^9.0.0",
    "@testing-library/dom": "^9.0.1",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.9",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "@vitejs/plugin-react": "^3.1.0",
    "autoprefixer": "^10.4.13",
    "babel-loader": "^9.1.0",
    "concurrently": "^7.6.0",
    "cypress": "^12.8.1",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest-dom": "^4.0.3",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0",
